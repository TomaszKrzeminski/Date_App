@model PairImagesViewResult


<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <h1>Jesteście Parą</h1>
        </div>
        <div class="col-lg-6">
            <img class="img-thumbnail" style="border-style:outset;min-width:160px;min-height:120px; max-height:480px;max-width:640px;" @*alt=@Model.match.PairMail*@ src=@Model.MyImagePath>
            <h3>@Model.MyEmail</h3>
        </div>
        <div class="col-lg-6">
            <img class="img-thumbnail" style="border-style:outset;min-width:160px;min-height:120px; max-height:480px;max-width:640px;" @*alt=@Model.match.PairMail*@ src=@Model.PairImagePath>
            <h3>@Model.PairEmail</h3>
        </div>
    </div>
</div>




