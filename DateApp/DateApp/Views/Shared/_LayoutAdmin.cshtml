<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <style type="text/css">
    </style>




    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />

    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>


    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.min.js"></script>
    <link rel="stylesheet" type="text/css" href="https://code.jquery.com/ui/1.12.0/themes/smoothness/jquery-ui.css">
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.validate/1.16.0/jquery.validate.min.js"></script>
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.validation.unobtrusive/3.2.6/jquery.validate.unobtrusive.min.js"></script>
    <style type="text/css">
        .table  {
            max-width: 400px;
        }

        
    </style>

</head>

<body>

    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-3">
                <form asp-action="Logout" asp-controller="Account">


                    <div>
                        <button type="submit" class=" btn btn-warning">Wyloguj</button>
                    </div>




                </form>
            </div>
            <div class="col-lg-3">
                <form asp-action="SchedulerDetails" asp-controller="Email">


                    <div>
                        <button type="submit" class=" btn btn-info">Planista</button>
                    </div>




                </form>
            </div>
        </div>
    </div>

    
    <div class="container-fluid">
        <div class="row">

            <div class="col-lg-4">

            </div>
            <div class="col-lg-6">
                <h1>Panel Administracyjny</h1>
            </div>
            <div class="col-lg-2 ">

            </div>

        </div>
        <div class="row ">



            @await Component.InvokeAsync(nameof(ShowLoginDetails))






        </div>


    </div>






    <div>
        @RenderBody()
    </div>
    <div>
        @RenderSection("scripts", false)
    </div>
</body>
</html>