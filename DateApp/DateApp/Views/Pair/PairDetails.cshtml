@model PairDetailsViewModel





<div class="container">
    <div class="row">
        <div class="col col-lg-3"></div>

        <div style=" min-height:1900px" class="col-lg-6 col-md-6  col-sm-8">
            <div class="row">
                <div style="min-height:610px;" class="col-lg-12">
                    <div class="row">
                        <div class="col-lg-6 col-md-6 ">
                            <a href="#">
                                <input type="hidden" asp-for="MainPhotoPath" />
                                <img class="imageR  img-thumbnail" src=@Model.MainPhotoPath alt="">

                            </a>
                        </div>
                        <div class="col-lg-6 col-md-6 ">
                            <a href="#">
                                <input type="hidden" asp-for="PhotoPath1" />
                                <img class="imageR  img-thumbnail" src=@Model.PhotoPath1 alt="">

                            </a>
                        </div>
                        <div class="col-lg-6 col-md-6 ">
                            <a href="#">
                                <input type="hidden" asp-for="PhotoPath2" />
                                <img class="imageR  img-thumbnail" src=@Model.PhotoPath2 alt="">

                            </a>
                        </div>
                        <div class="col-lg-6 col-md-6 ">
                            <a href="#">
                                <input type="hidden" asp-for="PhotoPath3" />
                                <img class="imageR  img-thumbnail" src=@Model.PhotoPath3 alt="">

                            </a>
                        </div>

                    </div>



                </div>
              
                <div class="row">

                    <div class="col-lg-12 form-group">
                        <label>Imię :</label>
                       <label>@Model.Name</label>
                    </div>
                    <div class="col-lg-12 form-group">
                        <label>Nazwisko :</label>
                        <label>@Model.Surname</label>
                       
                    </div>
                    <div class="col-lg-12 form-group">
                        <label>Email :</label>
                        <label>@Model.Email</label>
                      
                    </div>
                    <div class="col-lg-12 form-group">
                        <lablel asp-for="Sex">Płeć :</lablel>
                        <label>@Model.Sex</label>
                       
                    </div>
                    <div class="col-lg-12 form-group">
                        <label>Data urodzenia :</label>
                        <label>@Model.Dateofbirth</label>
                       
                    </div>
                    <div class="col-lg-12 form-group">
                        <label>Wiek :</label>
                        <label>@Model.Age</label>
                      
                    </div>
                    <div class="col-lg-12 form-group">
                        <label>Miasto :</label>
                        <label>@Model.City</label>
                       
                    </div>




                </div>

                <div style="min-height:200px; " class="col-lg-12 form-group">

                    <label class="font-weight-bold" asp-for="Description">Opis</label>
                    <input readonly style="height:140px" class="form-control" asp-for="Description" />

                </div>
                <div style="min-height:200px; " class="col-lg-12 form-group">

                    <label class="font-weight-bold" asp-for="JobPosition">Stanowisko</label>
                    <input readonly style="height:140px" class="form-control" asp-for="JobPosition" />

                </div>
                <div style="min-height:200px; " class="col-lg-12 form-group">

                    <label class="font-weight-bold" asp-for="CompanyName">Firma</label>
                    <input readonly style="height:140px" class="form-control" asp-for="CompanyName" />

                </div>
                <div style="min-height:200px; " class="col-lg-12 form-group">

                    <label class="font-weight-bold" asp-for="School">Szkoła</label>
                    <input readonly style="height:140px" class="form-control" asp-for="School" />

                </div>
                <div style="min-height:200px; " class="col-lg-12 form-group">

                    <label class="font-weight-bold" asp-for="CityOfResidence">Mieszkam w</label>
                    <input readonly style="height:140px" class="form-control" asp-for="CityOfResidence" />

                </div>
                <div>
                    <input type="hidden" asp-for="DetailsId" />
                </div>
                <div>
                    <input type="hidden" asp-for="UserId" />
                </div>


            </div>






        </div>


        <div class="col col-lg-3"></div>


    </div>



    
</div>





